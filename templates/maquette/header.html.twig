<div class="row">

    <div class="col-3">
        <img src="{{ asset('assets/maquette/LOGO_ACTUEL.png') }}">
    </div>

    <div class="col-9 d-flex flex-column align-items-start">
        <nav class="navbar navbar-expand-lg col-12 mt-auto">
            <ul class="navbar-nav w-100 justify-content-end">
                <li><img src="{{ asset('assets/maquette/artistes_header.png') }}"></li></li>
                {% for src in btns %}
                    {% set url = 'assets/maquette/' %}
                    {% set ext = '.png' %}
                    {% set src = url ~ src ~ ext %}
                    <li><img src="{{ asset(src) }}"></li>
                {% endfor %}
            </ul>
        </nav>

        <nav class="navbar navbar-expand-lg col-12 mb-auto">
            <ul class="navbar-nav w-100 justify-content-between">
                {% for name, url in data %}
                    <li class="border-md-right"><strong><a class="text-decoration-none" style="color:black; font-size:35px" href="{{ path(url) }}">{{ name|upper }}</a></strong></li>
                {% endfor %}
            </ul>
        </nav>
    </div>


</div>

<img class="w-100" src="{{ asset('assets/maquette/image_header.png') }}">

